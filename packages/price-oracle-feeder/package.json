{
  "name": "@spicyfi/price-oracle-feeder",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">= 22"
  },
  "scripts": {
    "start": "pnpm build && node build/index.js",
    "build": "tsc",
    "build:clean": "rm -rf ./build",
    "test": "node --stack-size=2000 --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --no-cache",
    "test:coverage": "pnpm test -- --coverage",
    "test:ci": "pnpm test -- --colors --coverage --ci",
    "lint": "eslint --ext .ts,.js src",
    "format": "prettier \"./**\" --write --ignore-unknown",
    "format:check": "prettier \"./**\" --ignore-unknown --check"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.30",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.7.0",
    "nock": "^13.5.6",
    "prettier": "^2.8.8",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "dotenv": "^16.4.7",
    "ethers": "^6.13.2"
  }
}
