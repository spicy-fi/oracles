{
  "name": "@spicyfi/price-oracle-feeder",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">= 22"
  },
  "scripts": {
    "start": "pnpm build && node build/index.js",
    "build": "tsc",
    "build:clean": "rm -rf ./build",
    "test": "node --stack-size=2000 --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --no-cache",
    "test:coverage": "pnpm test -- --coverage",
    "test:ci": "pnpm test -- --colors --coverage --ci",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "format": "biome format",
    "format:fix": "biome format --write",
    "check": "biome check",
    "check:fix": "biome check --write"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.0.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nock": "^13.5.6",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@biomejs/biome": "^1.9.4",
    "axios": "^1.8.4",
    "dotenv": "^16.4.7",
    "ethers": "^6.13.2"
  }
}
